<body>

    <%- include("../partials/nav", {browse: true, profile: false, photos: false, messages: false}) %>

  <main class="container">

    <div class="row">
      <div class="col s12 m12">
        <a href="/profiles/show/<%= userId%>" class="tooltipped" data-position="left" data-delay="100" data-tooltip="View Profile"><h4 class="photo-page-link center"><%= userName%></h4></a>
        <div class="carousel">
          <% photoArray.forEach((photo) => { %>
            <a class="carousel-item" href="/photos/showone/<%= photo.photoId%>"><img src="<%= photo.imagePath%>"></a>
          <% }); %>
        </div>
      </div>
    </div>
  </main>

